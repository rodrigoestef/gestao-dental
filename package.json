{
  "name": "gestao-dental",
  "version": "1.0.0",
  "main": "build/electron/index.js",
  "license": "MIT",
  "scripts": {
    "electron-watch": "tsc --watch -p main/tsconfig.json",
    "electron-start": "cross-env NODE_ENV=development nodemon -w build/ --exec \"electron . \" ",
    "electron": "concurrently \" yarn electron-watch \" \" yarn electron-start \" ",
    "react-start": "webpack --watch --mode=development",
    "start": "concurrently \" yarn electron\" \"yarn react-start\"",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "build": "electron-builder",
    "prebuild": "webpack --mode=production && tsc -p main/tsconfig.json"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@svgr/webpack": "^5.5.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.14.13",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.9",
    "notistack": "^1.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "style-loader": "^3.2.1",
    "styled-components": "^5.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "yup": "^0.32.9"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "puppeteer-core": "^13.0.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.37"
  },
  "build": {
    "icon": "build/electron/icon.png",
    "appId": "re.gentaodental.estef",
    "productName": "Gest√£o Dental",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      "chrome-win/**/*",
      "views/**/*"
    ],
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowToChangeInstallationDirectory": false
    }
  }
}