{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./rendered/components/*"
      ],
      "@assets/*": [
        "./rendered/assets/*"
      ],
      "@actions/*": [
        "./rendered/actions/*"
      ],
      "@reducers/*": [
        "./rendered/reducers/*"
      ],
      "@saga/*": [
        "./rendered/saga/*"
      ],
      "@services/*": [
        "./rendered/services/*"
      ],
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "rendered"
  ]
}